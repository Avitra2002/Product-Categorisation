    "import pandas as pd\n",
    "\n",
    "def is_valid_feedback(feedback):\n",
    "    if pd.isna(feedback):\n",
    "        return False\n",
    "    words = feedback.split()\n",
    "    return len(feedback) > 1 and len(words) > 2"
